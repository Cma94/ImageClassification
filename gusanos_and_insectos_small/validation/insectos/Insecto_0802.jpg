 		 var json_rr = JSON.parse(myString);
 	     console.log(json_rr);

 	var aa = 'AAAA ';
 	for (var i = 0; i < json_rr.vertices.length; i++) {

    //var myArticle = document.createElement('article');
    //var myP = document.createElement('p');
    //myP.textContent = json_rr[i].adress;
      aa = aa + json_rr.vertices[i].adress;
  	}
 	document.getElementById("demo").innerHTML = aa


	}
 }
